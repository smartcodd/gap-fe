div(id="chat_window_base",class=["row","chat-window"],style="margin-left:2px;display:nonde")
    +columna(10,10,12)
        div(class=["panel","panel-default"])
            div(class=["panel-heading","top-bar"])
                +columna(8,8,8)
                    h3(class=["panel-title"])
                        span(class=["glyphicon","glyphicon-comment"]) 
                        Chat - Miguel
                +columna(4,4,4,"text-align: right;")
                    a(href="#" )
                        span(class=["glyphicon","glyphicon-minus","icon_minim"])
                    a(href="#" )
                        span(class=["glyphicon","glyphicon-remove","icon_close"],data-id="chat_window_1")
            div(class=["panel-footer"])
                .input-group
                    input(type="text"  class=["form-control","input-sm","chat_input"] placeholder="Escribe tu mensaje aqui..s")
                    span(class="input-group-btn")
                    button( class=["btn", "btn-primary" ,"btn-sm","btn-chat"] ) Send

div(id="chat_window_1",class=["row","chat-window"],style="margin-left:2px;")
    +columna(10,10,12)
        div(class=["panel","panel-default"])
            div(class=["panel-heading","top-bar"])
                +columna(8,8,8)
                    h3(class=["panel-title"])
                        span(class=["glyphicon","glyphicon-comment"]) 
                        Chat - Miguel
                +columna(4,4,4,"text-align: right;")
                    a(href="#" )
                        span(class=["glyphicon","glyphicon-minus","icon_minim"])
                    a(href="#" )
                        span(class=["glyphicon","glyphicon-remove","icon_close"],data-id="chat_window_1")
            
            div(class=["panel-body","msg_container_base"] id="msg_container")
                if listMsg
                    for mensaje in listMsg
                        div(class=["row","msg_container","base_sent"])
                            +columna(10,10,10)
                                div(class=["messages","msg_sent"])
                                    p #{mensaje.msg}
                                    - var fecha=mensaje.fechaEnvio.getTime();
                                    time(datetime=mensaje.fechaEnvio.getTime()) Timothy • 51 min
                            +columna(2,2,2,"padding-left: 2px;padding-right: 2px;")
                                img(src="http://www.bitrebels.com/wp-content/uploads/2011/02/Original-Facebook-Geek-Profile-Avatar-1.jpg" class="img-responsive")

                script(id="msg_sent" type="text/x-handlebars-template" src="/docs/js/client.js")
                    div(class=["row","msg_container","base_sent"])
                        +columna(10,10,10)
                            div(class=["messages","msg_sent"])
                                p {{msg}}
                                time(datetime="2009-11-13T20:00") Timothy • 51 min
                        +columna(2,2,2,"padding-left: 2px;padding-right: 2px;")
                            img(src="http://www.bitrebels.com/wp-content/uploads/2011/02/Original-Facebook-Geek-Profile-Avatar-1.jpg" class="img-responsive")
                script(id="msg_receiver" type="text/x-handlebars-template" src="/docs/js/client.js")
                        div(class=["row","msg_container","base_receive"])
                        +columna(2,2,2,"padding-left: 2px;padding-right: 2px;")
                            img(src="http://www.bitrebels.com/wp-content/uploads/2011/02/Original-Facebook-Geek-Profile-Avatar-1.jpg" class="img-responsive")
                        +columna(10,10,10)
                            div(class=["messages","msg_smsg_receiveent"])
                                p {{msg}}
                                time(datetime="2009-11-13T20:00") Timothy • 51 min
            div(class=["panel-footer"])
                .input-group
                    input(type="text"   class=["form-control","input-sm","chat_input"] placeholder="Escribe tu mensaje aqui..s")
                    span(class="input-group-btn")
                    button(class=["btn", "btn-primary" ,"btn-sm","btn-chat"] ) Send
div(class=["btn-group","dropup"])
    button(type="button" class=["btn","btn-default","dropdown-toggle"]  data-toggle="dropdown")
        span(class=["glyphicon","glyphicon-cog"])
        span(class=["sr-only"]) Toggle Dropdown
    ul(class=["dropdown-menu"] role="menu")
        li
            a(href="#" id="new_chat") 
                span(class=["glyphicon","glyphicon-plus"])
                strong Nuevo Chat
        li(class=["divider"])
        li
            a(href="#" ) 
                span(class=["glyphicon","glyphicon-eye-close"])
                strong Invisivel
script(src="/docs/js/client.js")
